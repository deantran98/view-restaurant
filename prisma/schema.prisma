generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RestaurantRecord {
  id             String   @id @default(uuid())
  name           String
  category       String
  city           String
  desc           String
  rating         Float
  rating_count   Int
  price_range    String
  images         String[]
  featured       Json
  isFavorite     Boolean
}